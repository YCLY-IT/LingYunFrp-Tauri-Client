{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "lingyunfrp-tauri-client",
  "version": "0.1.46",
  "identifier": "凌云FRP",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "凌云FRP",
        "width": 1300,
        "height": 620,
        "decorations": false,
        "fullscreen": false,
        "resizable": true,
        "visible": true
      }
    ],
    "security": {
      "csp": "default-src 'self'"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "",
    "category": "Network",
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    },
    "resources": [
      "src-tauri/tauri.conf.json",
      "src-tauri/.env"
    ]
  }
}