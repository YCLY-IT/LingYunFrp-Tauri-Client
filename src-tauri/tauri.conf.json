{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "lingyunfrp-tauri-client",
  "version": "0.1.0",
  "identifier": "com.lingyunfrp.client",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "凌云FRP",
        "width": 1300,
        "height": 620,
        "decorations": false,
        "fullscreen": false,
        "resizable": true,
        "visible": true
      }
    ],
    "security": {
      "csp": "default-src 'self'"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "copyright": "",
    "category": "Network",
    "macOS": {
      "minimumSystemVersion": "10.13"
    },
    "deb": {
      "depends": ["libgtk-3-0", "libwebkit2gtk-4.0-37"]
    },
    "windows": {
      "wix": {
        "language": "zh-CN",
        "template": null
      },
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  }
}
